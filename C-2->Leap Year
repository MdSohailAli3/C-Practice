#include<stdio.h>
int main(int argc, char const *argv[])
{
    int n;
    printf("enter a year to check");
    scanf("%d",&n);
    if(n%4==0)
    {
        if (n%100==0)
        {
            if (n%400==0)
            {
                printf("Leap year");
            }
            else{printf("not a Leap year");}
        }
        else{printf(" Leap year");}
    }else{printf("not a Leap year");}
    return 0;
}
